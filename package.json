{
  "name": "nest-typeorm",
  "version": "1.36.0",
  "description": "NestJS app by typeorm and postgresql DB. My first steps work with NodeJS, Express, NestJS, Swagger, MongoDB, PostgreSQL, Microservices, RabbitMQ, WebSocket, DevOps basics (docker + kubernetes + jenkins), SOLID, Hexagonal architecture, SAGA,front-end basics.",
  "author": "Serge Hall",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": "20.11.1",
    "yarn": "4.1.1",
    "npm": "10.5.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/regiser -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -i --detectOpenHandles --forceExit --config ./test/jest-e2e.json",
    "generate-migrations": "node ./src/db/tools/generate-migration.js",
    "apply-migrations": "typeorm-ts-node-esm migration:run -d ./src/db/data-source.ts",
    "revert-last-migrations": "typeorm-ts-node-esm migration:revert -d ./src/db/data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@casl/ability": "^6.5.0",
    "@nestjs-modules/mailer": "1.10.3",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.7",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cron": "^2.4.0",
    "@types/joi": "^17.2.3",
    "@types/md5": "^2.3.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/stripe": "^8.0.417",
    "@types/uuid4": "^2.0.3",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "joi": "^17.12.2",
    "md5": "^2.3.0",
    "mongoose": "^8.2.1",
    "nodemailer": "^6.9.11",
    "npm": "^10.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "string-similarity-js": "^2.1.4",
    "stripe": "^14.21.0",
    "typeorm": "^0.3.20",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*(-|.)spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
